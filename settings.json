{
  "file_processing": {
    "extensions": {
      "valid": [".mkv", ".mp4", ".mpeg", ".mpg", ".mov", ".avi"],
      "excluded": [".part", "sample.mkv", "Sample.mkv", "trailer.mkv", "Trailer.mkv", ".DS_Store"],
      "deletable": [
        ".DS_Store",
        "RARBG.txt",
        ".srt",
        ".nfo",
        ".sfv",
        "Thumbs.db",
        ".txt",
        ".jpg",
        ".vtx"
      ]
    },
    "keywords": {
      "downloading": [".part"],
      "ignore": ["preview", "sample", "trailer"]
    }
  },
  "metadata": {
    "year_range": {
      "min": 1900,
      "max": 2030
    },
    "name_cleanup": {
      "flags": ["bbc", "pbs", "itv", "ch4", "ch5", "ng"]
    }
  },
  "libraries": {
    "shows": [
      {
        "name": "Documentaries",
        "path": "/mnt/media/documentaries"
      },
      {
        "name": "Television",
        "path": "/mnt/media/television"
      }
    ],
    "movies": [
      {
        "name": "Movies",
        "path": "/mnt/media/movies"
      }
    ],
    "music": [
      {
        "name": "Music",
        "path": "/mnt/media/music"
      }
    ]
  },
  "paths": {
    "default_source": "/mnt/media/transmission"
  }
}